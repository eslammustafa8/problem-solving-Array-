#include<bits\stdc++.h>

using namespace std;

void in_out()
{
    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie();
    #ifndef ONLINE_JUDGE
    freopen("in.txt","r",stdin);
    freopen("out.txt","w",stdout);
    freopen("out.txt","w",stderr);
    #endif
}

int main()
{
    in_out();

    int n;
    cin>>n;
    int arr[n]{0};
    for(int i=0;i<n;cin>>arr[i],i++);
    int value,indx=-1;
    cin>>value;
    for(int i=0;i<n;i++) if(arr[i]==value) {indx=i; break;}
    cout<<indx<<endl;
    return 0;
}
